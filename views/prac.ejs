<form class="card-body cardbody-color p-lg-5" id="userlogin">
        <div class="modal-body">
            <div class="form-title text-center">
                <h4>Login</h4>
            </div>
            <div class="d-flex flex-column text-center">
                <form>
                    <div class="form-control einput">
                        < <input type="email" class="form-control" name="email" id="email"
                                 data-rule-required="true"
                                 placeholder="Enter E-Mail"
                                 data-msg-required="*E-Mail Required">
                        <label for="">Email Id</label>
                    </div>
                    <div class="form-control pass-input">
                        <input type="password" class="form-control" name="password" id="password"
                               data-rule-required="true"
                               placeholder="Enter Password"
                               data-msg-required="*Password Required">
                        <label for="">Password</label>
                    </div>

                    <div id="container">
                        <input type="button" class="btn btn-color px-5 mb-5 w-100 text-center btn-userLogin"
                               onclick="UserLoginValues()" value="LOGIN"/>
                    </div>
                    <p class="text">Don't have a account?
                        <a href="/usersignup">Create Account</a>
                    </p>
                </form>
                <div id="error"></div>
            </div>
        </div>
</form>


