        // Navigation bar icon script
        $(document).ready(function () {
            $('.btn').click(function () {
                $('.items').toggleClass("show");
                $('ul li').toggleClass("hide");
            });
            var tooltipTriggerList = [].slice.call(document.querySelectorAll('[data-bs-toggle="tooltip"]'))
            var tooltipList = tooltipTriggerList.map(function (tooltipTriggerEl) {
                return new bootstrap.Tooltip(tooltipTriggerEl)
            })
        });

        const myslide2 = document.querySelectorAll('.myslide2'),
            dot1 = document.querySelectorAll('.dot2');
        let counter1 = 1;
        slidefun(counter1);
        let timer1 = setInterval(autoSlide, 3000);

        function autoSlide() {
            counter1 += 1;
            slidefun(counter1);
        }

        function plusSlides(n) {
            counter1 += n;
            slidefun(counter1);
            resetTimer();
        }

        function currentSlide(n) {
            counter1 = n;
            slidefun(counter1);
            resetTimer();
        }

        function resetTimer() {
            clearInterval(timer1);
            timer1 = setInterval(autoSlide, 8000);
        }

        function slidefun(n) {
            let i;
            for (i = 0; i < myslide2.length; i++) {
                myslide2[i].style.display = "none";
            }
            for (i = 0; i < dot1.length; i++) {
                dot1[i].className = dot1[i].className.replace(' active', '');
            }
            if (n > myslide2.length) {
                counter1 = 1;
            }
            if (n < 1) {
                counter1 = myslide2.length;
            }
            myslide2[counter1 - 1].style.display = "block";
            dot1[counter1 - 1].className += " active";
        }
    </script>


